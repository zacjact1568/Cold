<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circle_cnb"
        android:layout_width="@dimen/cnb_circle_size"
        android:layout_height="@dimen/cnb_circle_size"
        android:layout_gravity="center"
        tools:src="@color/colorPrimary"/>

    <ImageView
        android:id="@+id/icon_cnb"
        android:layout_width="@dimen/cnb_icon_size"
        android:layout_height="@dimen/cnb_icon_size"
        android:layout_gravity="center"
        tools:src="@drawable/ic_arrow_forward_white_24dp"/>

</merge>